{
    "version": 1,
    "disable_existing_loggers": "False",
    "formatters": {
        "console": {
            "format": "%(asctime)s %(name)-12s %(levelname)-8s %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.FileHandler",
            "formatter": "console",
            "filename": "somefile.txt"
        }
    },
    "loggers": {
        "": {
            "level": "DEBUG",
            "handlers": ["console"]
        }
    }
}